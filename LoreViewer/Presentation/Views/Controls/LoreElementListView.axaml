<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LoreViewer.Presentation.Views.Controls.LoreElementListView"
             xmlns:vm="using:LoreViewer.Presentation.ViewModels.Modes"
             x:DataType="vm:LoreReadonlyViewModel">
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="30"/> 
      <RowDefinition /> 
    </Grid.RowDefinitions>
    <TreeDataGrid Grid.Row="1" x:Name="OutlineList" Source="{Binding EntityTreeData}">
    </TreeDataGrid>
  </Grid>
</UserControl>
